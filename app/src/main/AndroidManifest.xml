<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="space.fedorenko.matrixcalculator">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_icon_matrix_calculator"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_icon_matrix_calculator_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MatrixCalculator">
        <activity android:name=".HelperActivity"></activity>
        <activity android:name=".AboutActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>